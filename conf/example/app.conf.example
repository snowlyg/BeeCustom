appname = BeeCustom
httpaddr = "127.0.0.1"
httpport = 8080
runmode = local

#Session参数
sessionon =true
sessionname = rmsbackusersessionid
sessionprovider = redis
sessionproviderconfig = "127.0.0.1:6379"
sessioncookielifetime = 100 #cookie的有效期（秒），默认是session,即会话结束则过期

#开启监控
EnableAdmin = true
#开启JSON请求
copyrequestbody = true
#防止跨站请求伪造 未启用
enablexsrf = true
xsrfkey = asdfioerjlasdfmFADFOEJF2323SDFF
xsrfexpire = 3600

#数据库类型：postgres/mysql/sqlite3
db_type = "mysql"
#数据库表名前辍
db_dt_prefix = "bee_custom_"

#postgreSQL配置copyrequestbody
[postgres]
db_alias = "default"
db_name = "sdrms"
db_user = "root"
db_pwd = "sdsdsd"
db_host = "127.0.0.1"
db_port = 5432
db_sslmode = "disable"

#mysql配置
[mysql]
db_alias = "default"
db_name = "bee_custom"
db_user = "root"
db_pwd = "nvWQ8qE6kUtSURHhSQvWa2BZ3ct0eDOo"
db_host = "127.0.0.1"
db_port = 3306
db_charset = "utf8"

#sqlite3
[sqlite3]
db_alias = "default"
db_name = "sdrms.sqlite3"

[lang]
types=en-US|zh-CN
names=English|简体中文

#日志配置
[logs]
#"emergency", "alert", "critical", "error", "warning", "notice", "info", "debug"
# 0 ~ 7
level = 7

#cache配置
[cache]
#redis
redis_host = "127.0.0.1:6379"
redis_password = ""
#过期时间
cache_expire = 10
























